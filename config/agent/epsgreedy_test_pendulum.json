{
    "agent_name": "epsgreedy",
    "parameters":
    {
        "clip_actions": [true],
        "replay_capacity": [100000],
        "reparam_baseline": [[true, 0]],
        "batch_size": [32],
        "tau": [0.01],
        "policy_type": ["DeterministicAction"],
        "target_update_interval": [1],
        "critic_lr": [1e-2],
        "actor_lr_scale": [0.1],
        "hidden_dim": [64],
        "weight_init": ["xavier_uniform"],
        "uniform_exploration_steps": [0],
        "steps_before_learning": [1000],
        "cuda": [true],
        "epsilon": [0.1],
        "epsilon_decay": [0.995],
        "epsilon_min": [0.1]
    }
}
